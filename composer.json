{
    "name": "cakesuit/option",
    "description": "Define/fetching quickly preference for your application",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric KOLLER",
            "email": "labelsuisse@gmail.com"
        }
    ],
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeSuit\\Option\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeSuit\\Option\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
